<h2>Log in</h2>
<br>
<%= form_tag sessions_path do %>
    <p>
      <%= label_tag :email %>
      <%= email_field_tag :email, params[:email] %>
    </p>
    <p>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </p>
    <p><%= submit_tag "Log in" %></p>
<% end %>

<p><%= link_to "Still no account? Then sign up here!", new_user_account_path %></p>