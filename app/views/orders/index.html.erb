<h1>Orders#index</h1>
<p>Find me in app/views/orders/index.html.erb</p>

<table>
  <thead>
  <tr>
    <th>Tischnummer</th>
    <th>Bestellnummer</th>
  </tr>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
      <tr>
        <td><%= order.table.table_number %></td>
        <td><%= link_to order.id, order %> </td>
      </tr>
  <% end %>
  </tbody>
</table>

